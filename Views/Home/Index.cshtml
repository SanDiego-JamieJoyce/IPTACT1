@model StudentInfoModel[]
<style>
.student-table {
    width: 100%
}
.student-table th {
border: 1px solid rgb(109, 105, 227);
padding: 8px 12px;
background: #e274d3;
}
.student-table td {
padding: 4px 8px;
border: 1px solid #8492d0a1;
background-color: #9fe0ffa1;
}

</style>

<h1>Sample Student Information</h1>

<table border="1" class="student-table">
    <thead>
        <tr>
            <th>Name</th>
            <th>Student ID</th>
            <th>Age</th>
            <th>Hobby</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var student in Model)
        {
            <tr>
                <td>@student.Name</td>
                <td>@student.StudentId</td>
                <td>@student.StudentAge</td>
                <td>@student.StudentHobby</td>
            </tr>
        }
    </tbody>
</table>
